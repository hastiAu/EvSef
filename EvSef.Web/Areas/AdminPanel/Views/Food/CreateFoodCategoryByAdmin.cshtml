@model EvSef.Domain.ViewModels.FoodCategory.CreateFoodCategoryViewModel
 

@{
    ViewData["Title"] = "CreateFoodCategoryByAdmin";
}


<div class="row page-titles mx-0">
    <div class="col p-md-0">
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a asp-controller="home" asp-action="index">Dashboard</a></li>
            <li class="breadcrumb-item active"><a href="javascript:void(0)">Create Food Category</a></li>
        </ol>
    </div>
</div>
<div class="content-body">

    <div class="container-fluid mt-3">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="card">
                    <div class="card-body">
                        <div class="basic-form">
                            <form asp-controller="Food" asp-action="CreateFoodCategoryByAdmin" method="post" enctype="multipart/form-data">
                                <div class="form-row"/>
                                <div class="form-group col-md-6">
                                    <label asp-for="FoodCategoryTitle">Food Category Title</label>
                                    <input asp-for="FoodCategoryTitle" type="text" class="form-control" >
                                    <span asp-validation-for="FoodCategoryTitle"></span>
                                </div>

                                <div class="form-check ml-5 mt-5 mb-3">
                                        <input asp-for="IsActive" class="form-check-input" type="checkbox">
                                        <label class="form-check-label">Active</label>
                                    </div>
                              
                                <div class="col-md-12 col-sm-12">
                                    <div class=" ">
                                        <button type="submit" class="button-body btn mb-1 btn-lg custom-btn-create">Create Food Category </button>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

